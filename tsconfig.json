{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "importsNotUsedAsValues": "error",
    "types": ["node"],
    "jsx": "react-jsx"
  },
  "extends": "astro/tsconfigs/strict"
}
